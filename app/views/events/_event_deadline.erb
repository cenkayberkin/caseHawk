<li id="<%= event.id %>" data-event-id="<%= event.id %>" data-name="<%= event.name %>" data-type="<%= event.type %>" 
  data-starts-at="<%= event.starts_at %>" data-starts-at-time="<%= event.starts_at_time %>"
  class="event event_completable deadline <%= event.completed_at ? "complete" : "incomplete" %>">
  <% if 0 == 1 %>
    <%= check_box_tag event.id, '1', event.completed_at, :id => "event-complete-#{event.id}", :class => 'task', :name => event.id %>
  <% end %>
  <a rel="facebox" href="#event-details_<%= event.id %>" class="event-title">
    <%= event.starts_at.to_s(:simple) + ": " + event.name %>
  </a>
  <div class="event-details details_<%= event.id %>" id="event-details_<%= event.id %>" >
    <%= render :partial => "events/details", :locals => { :event => event } %>
  </div>
</li>
