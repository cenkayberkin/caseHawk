<div class='list'>
  <%= link_to 'Add note', notes_case_path(format: :html), :class => 'add_note', :remote => true %>

  <ul>
    <% kase.notes.each do |note| %>
      <li>
        <%= link_to note.title, '#', :class => 'note', :'data-id' => note.id %>
      </li>
    <% end %>
  </ul>
</div>

<div class='notes'>
  <ul>
    <%= f.fields_for :notes do |builder| %>
      <%= render :partial => 'note_fields', :locals => { :f => builder } %>
    <% end %>
  </ul>
</div>
