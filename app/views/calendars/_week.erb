<table class="week" id="week-<%= @date.year %>-<%= @date.cweek %>">
  <thead>

  </thead>
  <tbody>
    <% render :partial => 'week_dates_header', 
                :locals => { :week_num => @date.cweek } %>
    <% render :partial => 'week_allday', 
                :locals => { :week_num => @date.cweek, :events => get_all_days(@events) } %>
    <% render :partial => 'week_day_full', 
                :locals => { :week_num => @date.cweek, 
                              :deadlines => get_deadlines(@events), 
                              :appointments => get_appointments(@events) } %>
  </tbody>
</table>
