<tr class="viewport week-day-full">
    <td class="week-hours">
      <ul class="day-hours">
        <%= render :partial => 'calendars/timeline' %>
      </ul>
    </td>
    <% (@date.beginning_of_week..@date.end_of_week).each do |day| %>
    <td class="day day-<%= day.wday %>" data-date="<%= day.to_s %>">
      <ul class="day-full collidable">
        <%= render :partial => "events/event", 
              :collection => (get_timed_events(@events)).select{ |e| e.starts_at.to_date == day } %>
      </ul>
    </td>
    <% end %>
</tr>
